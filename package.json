{
  "name": "@selfpub/selfpub-ui",
  "version": "1.8.0",
  "description": "Design system implementation. Component library Selfpub",
  "homepage": "https://mybook.github.io/selfpub-ui/",
  "main": "src/components/index.js",
  "author": "Ilya Reshetnikov <inbox@devxom.tech>",
  "license": "MIT",
  "config": {
    "components": "src/components/index.js"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "contributors": [
    "selfpub"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://github.com/mybook/selfpub-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mybook/selfpub-ui/issues"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "start": "yarn build && yarn run storybook:dev",
    "prebuild": "rm -rf lib",
    "build": "cross-env BABEL_ENV=production babel src/components --out-dir lib",
    "copy": "cp package.json .npmignore README.md lib",
    "deploy-storybook": "storybook-to-ghpages",
    "prepub": "npm run build",
    "pub": "npm publish --access public",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.out/jest-test-results.json",
    "test:watch": "jest --watch",
    "lint:js": "eslint --ext .js ./src/components/**/*.js"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@selfpub/babel-preset": "^1.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-jest": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-options": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.18",
    "@types/storybook__react": "^4.0.1",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "styled-components": "^4.2.0",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "polished": "^2.0.3",
    "query-string": "^6.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/utils/jest_setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "components",
    "design",
    "ui",
    "selfpub",
    "styled-components"
  ]
}
