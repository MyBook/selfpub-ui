{
  "name": "@selfpub/selfpub-ui",
  "version": "1.7.2",
  "description": "Design system implementation. Component library Selfpub",
  "homepage": "https://mybook.github.io/selfpub-ui/",
  "main": "src/components/index.js",
  "author": "selfpub.ru",
  "license": "MIT",
  "config": {
    "components": "src/components/index.js"
  },
  "engines": {
    "node": ">=9.1.1"
  },
  "contributors": [
    "selfpub"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://github.com/mybook/selfpub-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mybook/selfpub-ui/issues"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "start": "yarn build && yarn run storybook:dev",
    "prebuild": "rm -rf lib",
    "build": "cross-env BABEL_ENV=production babel src/components --out-dir lib",
    "copy": "cp package.json .npmignore README.md lib",
    "deploy-storybook": "storybook-to-ghpages",
    "prepub": "npm run build",
    "pub": "npm publish --access public",
    "storybook:build": "yarn run test:generate-output && build-storybook -c .storybook -o .out",
    "storybook:dev": "yarn run test:generate-output && start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.out/jest-test-results.json",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.5.0",
    "react-dom": "^15.3.0 || ^16.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-jest": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "polished": "^2.0.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "styled-components": "^3.4.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/utils/jest_setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components"
    ]
  },
  "keywords": [
    "react",
    "ui"
  ]
}
