{
  "name": "@selfpub/selfpub-ui",
  "version": "1.9.0",
  "description": "Design system components. Selfpub command element library",
  "homepage": "https://mybook.github.io/selfpub-ui/",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/selfpub-ui.umd.js",
  "files": [
    "dist"
  ],
  "author": "Ilya Reshetnikov <ilya.reshetnikov@devxom.tech>",
  "license": "MIT",
  "config": {
    "components": "src/components/index.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/mybook/selfpub-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mybook/selfpub-ui/issues"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "start": "yarn run storybook:dev",
    "build": "NODE_ENV=\"production\" rollup -c",
    "develop": "NODE_ENV=\"develop\" rollup -c -w",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:dev": "NODE_ENV=\"develop\" start-storybook -p 9001 -c .storybook",
    "yarn ": "NODE_ENV=\"develop\" start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:js": "eslint --ext .js ./src/components/**/*.js",
    "now-build": "yarn run build && yarn run storybook:build"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@selfpub/babel-preset": "^1.1.1",
    "@selfpub/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-jest": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-options": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/storybook__react": "^4.0.2",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.2.2",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-embed-css": "^1.0.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.1",
    "styled-components": "^4.3.2"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "polished": "^3.4.1",
    "query-string": "^6.8.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/utils/jest_setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "components",
    "design",
    "ui",
    "selfpub",
    "styled-components"
  ]
}
